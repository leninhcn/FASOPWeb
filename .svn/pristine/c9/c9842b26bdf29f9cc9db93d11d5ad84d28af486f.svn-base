 aria-haspopup=\"true\"\r\n                  aria-expanded=\"false\"\r\n                >\r\n                  <flag :iso=\"lang.code\" /> {{ lang.detail }}\r\n                </button>\r\n                <div class=\"dropdown-menu\">\r\n                  <a\r\n                    class=\"dropdown-item\"\r\n                    href=\"#\"\r\n                    @click.prevent=\"ChangeLanguage('us')\"\r\n                    ><flag iso=\"us\" /> English</a\r\n                  >\r\n                  <a\r\n                    class=\"dropdown-item\"\r\n                    href=\"#\"\r\n                    @click.prevent=\"ChangeLanguage('cn')\"\r\n                    ><flag iso=\"cn\" /> 简体中文</a\r\n                  >\r\n                  <a\r\n                    class=\"dropdown-item\"\r\n                    href=\"#\"\r\n                    @click.prevent=\"ChangeLanguage('vn')\"\r\n                    ><flag iso=\"vn\" /> Việt Nam</a\r\n                  >\r\n                </div>\r\n              </div>\r\n              <div class=\"row align-items-center remember\">\r\n                <input type=\"checkbox\" />{{ LoginLang.remember }}\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <input\r\n                  type=\"submit\"\r\n                  :value=\"LoginLang.login\"\r\n                  class=\"btn float-right login_btn\"\r\n                />\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import \"../../public/bootstrap/css/bootstrap-grid.min.css\"\r\n// import \"../../public/bootstrap/css/bootstrap-reboot.min.css\"\r\n// import \"../../public/bootstrap/css/bootstrap.min.css\"\r\n// import \"../../public/bootstrap/js/jquery.min.js\"\r\n// import \"../../public/bootstrap/js/bootstrap.bundle.min.js\"\r\n// import \"../../public/bootstrap/js/bootstrap.js\"\r\nimport \"@fortawesome/fontawesome-free/css/all.min.css\";\r\nimport { mapActions, mapGetters } from \"vuex\";\r\nexport default {\r\n  computed: {\r\n    ...mapGetters({\r\n      GetUser: \"getUser\",\r\n    }),\r\n  },\r\n  components: {},\r\n  name: \"login-app\",\r\n  data() {\r\n    return {\r\n      lang: { code: \"us\", detail: \"English\" },\r\n      LoginLang: {\r\n        sign_in: \"Sign In\",\r\n        user_name: \"User Name\",\r\n        pass_word: \"Password\",\r\n        login: \"Login\",\r\n        remember: \"Remember Me\",\r\n      },\r\n      user: {\r\n        UserName: \"\",\r\n        Password: \"\",\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.ChangeLanguage(this.lang_code);\r\n  },\r\n  methods: {\r\n    ...mapActions([\"Login\"]),\r\n    ChangeLanguage: function (lg) {\r\n      this.lang.code = lg;\r\n      switch (lg) {\r\n        case \"us\":\r\n          this.LoginLang = {\r\n            sign_in: \"Sign In\",\r\n            user_name: \"User Name\",\r\n            pass_word: \"Password\",\r\n            login: \"Login\",\r\n            remember: \"Remember Me\",\r\n          };\r\n          this.lang.detail = \"English\";\r\n          break;\r\n        case \"cn\":\r\n          this.LoginLang = {\r\n            sign_in: \"登入\",\r\n            user_name: \"用户\",\r\n            pass_word: \"密码\",\r\n            login: \"登录\",\r\n            remember: \"记得我\",\r\n          };\r\n          this.lang.detail = \"简体中文\";\r\n          break;\r\n        case \"vn\":\r\n          this.LoginLang = {\r\n            sign_in: \"Đăng Nhập\",\r\n            user_name: \"Tài Khoản\",\r\n            pass_word: \"Mật Khẩu\",\r\n            login: \"Đăng Nhập\",\r\n            remember: \"Ghi Nhớ Đăng Nhập\",\r\n          };\r\n          this.lang.detail = \"Việt Nam\";\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n    },\r\n    CheckLogin: function () {\r\n      const loading = this.$Loading.service({\r\n        lock: true,\r\n        text: \"Loading\",\r\n     