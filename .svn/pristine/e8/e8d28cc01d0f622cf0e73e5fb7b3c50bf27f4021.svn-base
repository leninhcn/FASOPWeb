abel=\"line.name\" glossy color=\"orange\" text-color=\"black\">\r\n          <q-menu\r\n            transition-show=\"scale\"\r\n            transition-hide=\"scale\"\r\n            class=\"bg-purple text-white\"\r\n            auto-close\r\n          >\r\n            <q-list\r\n              style=\"min-width: 100px\"\r\n              v-for=\"(el, id) in lines\"\r\n              :key=\"id\"\r\n            >\r\n              <q-item clickable @click=\"onLineChanged(el)\">\r\n                <q-item-section>{{ el.name }}</q-item-section>\r\n              </q-item>\r\n            </q-list>\r\n          </q-menu>\r\n        </q-btn>\r\n        <div style=\"font-size: 1.7em\" class=\"bg-orange btnArrow\">\r\n          <q-icon\r\n            :name=\"cltMenu ? 'double_arrow' : 'keyboard_double_arrow_left'\"\r\n          ></q-icon>\r\n        </div>\r\n      </q-btn-group>\r\n    </div>\r\n    <div ref=\"mainPn\" style=\"width: 100%\" class=\"mainPn\" id=\"mainPn\">\r\n      <div class=\"absolute-top-right z-top\">\r\n        <q-btn\r\n          @click=\"toggle\"\r\n          :icon=\"fullScreen ? 'fullscreen_exit' : 'fullscreen'\"\r\n          flat\r\n        ></q-btn>\r\n      </div>\r\n      <tr>\r\n        <td style=\"min-width: 500px\">\r\n          <WoTarget ref=\"WoTarget\"></WoTarget>\r\n        </td>\r\n        <td style=\"min-width: 500px; width: 100%\">\r\n          <OutputRate ref=\"OutputRate\"></OutputRate>\r\n        </td>\r\n      </tr>\r\n      <tr>\r\n        <td></td>\r\n        <td><StationYield ref=\"StationYield\"></StationYield></td>\r\n      </tr>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport OutputRate from \"./OutputRate.vue\";\r\nimport WoTarget from \"./WoTarget.vue\";\r\nimport StationYield from \"./StationYield.vue\";\r\n\r\nimport { AppFullscreen } from \"quasar\";\r\n\r\nexport default {\r\n  name: \"dash-board\",\r\n  components: {\r\n    OutputRate,\r\n    WoTarget,\r\n    StationYield,\r\n  },\r\n  data() {\r\n    ret