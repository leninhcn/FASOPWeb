{"ast":null,"code":"import Snap from 'snapsvg';\nexport default {\n  data() {\n    return {\n      s: Snap(document.getElementById(\"clock\")),\n      seconds: this.s.select(\"#seconds\"),\n      minutes: this.s.select(\"#minutes\"),\n      hours: this.s.select(\"#hours\"),\n      rim: this.s.select(\"#rim\"),\n      face: {\n        elem: this.s.select(\"#face\"),\n        cx: this.s.select(\"#face\").getBBox().cx,\n        cy: this.s.select(\"#face\").getBBox().cy\n      },\n      angle: 0,\n      easing: function (a) {\n        return a == !!a ? a : Math.pow(4, -10 * a) * Math.sin((a - .075) * 2 * Math.PI / .3) + 1;\n      },\n      sshadow: this.seconds.clone(),\n      mshadow: this.minutes.clone(),\n      hshadow: this.hours.clone(),\n      rshadow: this.rim.clone(),\n      shadows: [this.sshadow, this.mshadow, this.hshadow],\n      filter: Snap.filter.blur(0.1) + Snap.filter.brightness(0)\n    };\n  },\n  methods: {\n    setSeconds(t) {\n      t = t.getSeconds();\n      t %= 60;\n      var angle = t * 360 / 60;\n      //if ticking over to 0 seconds, animate angle to 360 and then switch angle to 0\n      if (angle === 0) angle = 360;\n      seconds.animate({\n        transform: \"rotate(\" + angle + \" \" + face.cx + \" \" + face.cy + \")\"\n      }, 600, easing, function () {\n        if (angle === 360) {\n          seconds.attr({\n            transform: \"rotate(\" + 0 + \" \" + face.cx + \" \" + face.cy + \")\"\n          });\n          sshadow.attr({\n            transform: \"translate(0, 2) rotate(\" + 0 + \" \" + face.cx + \" \" + face.cy + 2 + \")\"\n          });\n        }\n      });\n      sshadow.animate({\n        transform: \"translate(0, 2) rotate(\" + angle + \" \" + face.cx + \" \" + face.cy + 2 + \")\"\n      }, 600, easing);\n    },\n    setMinutes(t) {\n      var minute = t.getMinutes();\n      minute %= 60;\n      minute += Math.floor(t.getSeconds() / 10) / 6;\n      var angle = minute * 360 / 60;\n      minutes.animate({\n        transform: \"rotate(\" + angle + \" \" + face.cx + \" \" + face.cy + \")\"\n      }, 100, mina.linear, function () {\n        if (angle === 360) {\n          minutes.attr({\n            transform: \"rotate(\" + 0 + \" \" + face.cx + \" \" + face.cy + \")\"\n          });\n          mshadow.attr({\n            transform: \"translate(0, 2) rotate(\" + 0 + \" \" + face.cx + \" \" + face.cy + 2 + \")\"\n          });\n        }\n      });\n      mshadow.animate({\n        transform: \"translate(0, 2) rotate(\" + angle + \" \" + face.cx + \" \" + face.cy + 2 + \")\"\n      }, 100, mina.linear);\n    },\n    setHours(t) {\n      var hour = t.getHours();\n      hour %= 12;\n      hour += Math.floor(t.getMinutes() / 10) / 6;\n      var angle = hour * 360 / 12;\n      hours.animate({\n        transform: \"rotate(\" + angle + \" 244 251)\"\n      }, 100, mina.linear, function () {\n        if (angle === 360) {\n          hours.attr({\n            transform: \"rotate(\" + 0 + \" \" + face.cx + \" \" + face.cy + \")\"\n          });\n          hshadow.attr({\n            transform: \"translate(0, 2) rotate(\" + 0 + \" \" + face.cx + \" \" + face.cy + 2 + \")\"\n          });\n        }\n      });\n      hshadow.animate({\n        transform: \"translate(0, 2) rotate(\" + angle + \" \" + face.cx + \" \" + face.cy + 2 + \")\"\n      }, 100, mina.linear);\n    },\n    update() {\n      var time = new Date();\n      setHours(time);\n      setMinutes(time);\n      setSeconds(time);\n    }\n  }\n};","map":{"version":3,"names":["Snap","data","s","document","getElementById","seconds","select","minutes","hours","rim","face","elem","cx","getBBox","cy","angle","easing","a","Math","pow","sin","PI","sshadow","clone","mshadow","hshadow","rshadow","shadows","filter","blur","brightness","methods","setSeconds","t","getSeconds","animate","transform","attr","setMinutes","minute","getMinutes","floor","mina","linear","setHours","hour","getHours","update","time","Date"],"sources":["D:\\LearnVueJS\\Vue3\\Projec