{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport GetOption from \"./Datas/topbyStation\";\nexport default {\n  data() {\n    return {\n      myChart: undefined,\n      option: GetOption(),\n      colors: [\"#04B404\", \"orange\", \"yellow\", \"red\", \"indigo\", \"purple\", \"#cccccc\"]\n    };\n  },\n  mounted() {\n    var dom = document.getElementById(\"ecircle\");\n    this.myChart = this.$Echart.init(dom, null, {\n      renderer: \"canvas\",\n      useDirtyRect: false\n    });\n    if (this.option && typeof this.option === \"object\") {\n      this.myChart.setOption(this.option);\n    }\n    window.addEventListener(\"resize\", this.myChart.resize);\n    this.DataUpdate({\n      PDLINE_ID: \"10003\",\n      DB: \"BU20\"\n    });\n  },\n  methods: {\n    getChartData(iData) {\n      const data = [];\n      for (let id = 0; id < iData.length; id++) {\n        const el = iData[id];\n        data.push({\n          value: el.QTY,\n          name: el.DEFECT_CODE\n        });\n      }\n      return data;\n    },\n    addgraphic(value, xx, yy) {\n      var res = {\n        type: \"text\",\n        style: {\n          text: value,\n          x: xx,\n          y: yy,\n          fill: \"#FFF\",\n          //fontSize: 25,\n          font: \"bold 16px verdana\"\n        }\n      };\n      return res;\n    },\n    DataUpdate(iLine) {\n      this.myChart.clear();\n      this.axios.post(\"api/FATPKanban/GetTOP5FailByStation\", iLine).then(res => {\n        if (res.data.status == \"OK\") {\n          console.log(res.data.data);\n          const sr = [];\n          const grap = [];\n          var bar = this.myChart.getWidth() / res.data.data.DATA.length;\n          for (let i = 0; i < res.data.data.DATA.length; i++) {\n            const el = res.data.data.DATA[i];\n            const xx = i * bar + (bar - 10) / 2;\n            grap.push(this.addgraphic(el.PROCESS_NAME, xx, 240));\n            sr.push({\n              name: el.PROCESS_NAME,\n              type: \"pie\",\n              radius: \"45%\",\n              center: [`${i * 20 + 10}%`, \"45%\"],\n              color: this.colors,\n              label: {\n                normal: {\n                  formatter: \"{b}:{c}\",\n                  show: true,\n                  position: \"outside\"\n                }\n              },\n              labelLine: {\n                normal: {\n                  show: true\n                }\n              },\n              data: this.getChartData(el.INFO)\n            });\n          }\n          this.option.series = sr;\n          this.option.graphic = grap;\n          this.myChart.setOption(this.option);\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"names":["GetOption","data","myChart","undefined","option","colors","mounted","dom","document","getElementById","$Echart","init","renderer","useDirtyRect","setOption","window","addEventListener","resize","DataUpdate","PDLINE_ID","DB","methods","getChartData","iData","id","length","el","push","value","QTY","name","DEFECT_CODE","addgraphic","xx","yy","res","type","style","text","x","y","fill","font","iLine","clear","axios","post","then","status","console","log","sr","grap","bar","getWidth","DATA","i","PROCESS_NAME","radius","center","color","label","normal","formatter","show","position","labelLine","INFO","series","graphic","catch","err"],"sources":["D:\\LearnVueJS\\Vue3\\Projects\\fa-sop-online\\src\\components\\FATPkanban\\TopFailByStation.vue"],"sourcesContent":["<template>\r\n  <div id=\"ecircle\"></div>\r\n</template>\r\n\r\n<script>\r\nimport GetOption from \"./Datas/topbyStation\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      myChart: undefined,\r\n      option: GetOption(),\r\n      colors: [\r\n        \"#04B404\",\r\n        \"orange\",\r\n        \"yellow\",\r\n        \"red\",\r\n        \"indigo\",\r\n        \"purple\",\r\n        \"#cccccc\",\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    var dom = document.getElementById(\"ecircle\");\r\n    this.myChart = this.$Echart.init(dom, null, {\r\n      renderer: \"canvas\",\r\n      useDirtyRect: false,\r\n    });\r\n\r\n    if (this.option && typeof this.option === \"object\") {\r\n      this.myChart.setOption(this.option);\r\n    }\r\n    window.addEventListener(\"resize\", this.myChart.resize);\r\n    this.DataUpdate({ PDLINE_ID: \"10003\", DB: \"BU20\" });\r\n  },\r\n  methods: {\r\n    getChartData(iData) {\r\n      const data = [];\r\n      for (let id = 0; id < iData.length; id++) {\r\n        const el = iData[id];\r\n        data.push({ value: el.QTY, name: el.DEFECT_CODE });\r\n      }\r\n      return data;\r\n    },\r\n    addgraphic(value, xx, yy) {\r\n      var res = {\r\n        type: \"text\",\r\n        style: {\r\n          text: value,\r\n          x: xx,\r\n          y: yy,\r\n          fill: \"#FFF\",\r\n          //fontSize: 25,\r\n          font: \"bold 16px verdana\",\r\n        },\r\n      };\r\n      return res;\r\n    },\r\n    DataUpdate(iLine) {\r\n      this.myChart.clear();\r\n      this.axios\r\n        .post(\"api/FATPKanban/GetTOP5FailByStation\", iLine)\r\n        .then((res) => {\r\n          if (res.data.status == \"OK\") {\r\n            console.log(res.data.data);\r\n            const sr = [];\r\n            const grap = [];\r\n            var bar = this.myChart.getWidth() / res.data.data.DATA.length;\r\n            for (let i = 0; i < res.data.data.DATA.length; i++) {\r\n              const el = res.data.data.DATA[i];\r\n              const xx = i * bar + (bar-10) / 2;\r\n              grap.push(this.addgraphic(el.PROCESS_NAME, xx, 240));\r\n              sr.push({\r\n                name: el.PROCESS_NAME,\r\n                type: \"pie\",\r\n                radius: \"45%\",\r\n                center: [`${i * 20 + 10}%`, \"45%\"],\r\n                color: this.colors,\r\n                label: {\r\n                  normal: {\r\n                    formatter: \"{b}:{c}\",\r\n                    show: true,\r\n                    position: \"outside\",\r\n                  },\r\n                },\r\n                labelLine: {\r\n                  normal: {\r\n                    show: true,\r\n                  },\r\n                },\r\n                data: this.getChartData(el.INFO),\r\n              });\r\n            }\r\n            this.option.series = sr;\r\n            this.option.graphic = grap;\r\n            this.myChart.setOption(this.option);\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n#ecircle {\r\n  height: 30vh;\r\n  width: 100%;\r\n  overflow: hidden;\r\n}\r\n</style>"],"mappings":";AAKA,OAAOA,SAAQ,MAAO,sBAAsB;AAC5C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEC,SAAS;MAClBC,MAAM,EAAEJ,SAAS,EAAE;MACnBK,MAAM,EAAE,CACN,SAAS,EACT,QAAQ,EACR,QAAQ,EACR,KAAK,EACL,QAAQ,EACR,QAAQ,EACR,SAAS;IAEb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAIC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,SAAS,CAAC;IAC5C,IAAI,CAACP,OAAM,GAAI,IAAI,CAACQ,OAAO,CAACC,IAAI,CAACJ,GAAG,EAAE,IAAI,EAAE;MAC1CK,QAAQ,EAAE,QAAQ;MAClBC,YAAY,EAAE;IAChB,CAAC,CAAC;IAEF,IAAI,IAAI,CAACT,MAAK,IAAK,OAAO,IAAI,CAACA,MAAK,KAAM,QAAQ,EAAE;MAClD,IAAI,CAACF,OAAO,CAACY,SAAS,CAAC,IAAI,CAACV,MAAM,CAAC;IACrC;IACAW,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACd,OAAO,CAACe,MAAM,CAAC;IACtD,IAAI,CAACC,UAAU,CAAC;MAAEC,SAAS,EAAE,OAAO;MAAEC,EAAE,EAAE;IAAO,CAAC,CAAC;EACrD,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,KAAK,EAAE;MAClB,MAAMtB,IAAG,GAAI,EAAE;MACf,KAAK,IAAIuB,EAAC,GAAI,CAAC,EAAEA,EAAC,GAAID,KAAK,CAACE,MAAM,EAAED,EAAE,EAAE,EAAE;QACxC,MAAME,EAAC,GAAIH,KAAK,CAACC,EAAE,CAAC;QACpBvB,IAAI,CAAC0B,IAAI,CAAC;UAAEC,KAAK,EAAEF,EAAE,CAACG,GAAG;UAAEC,IAAI,EAAEJ,EAAE,CAACK;QAAY,CAAC,CAAC;MACpD;MACA,OAAO9B,IAAI;IACb,CAAC;IACD+B,UAAUA,CAACJ,KAAK,EAAEK,EAAE,EAAEC,EAAE,EAAE;MACxB,IAAIC,GAAE,GAAI;QACRC,IAAI,EAAE,MAAM;QACZC,KAAK,EAAE;UACLC,IAAI,EAAEV,KAAK;U