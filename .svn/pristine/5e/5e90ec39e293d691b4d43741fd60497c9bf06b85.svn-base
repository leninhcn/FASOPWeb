\r\n        delete() {\r\n            this.showMenu = false\r\n        },\r\n        showContextMenu(event, onode, dd) {\r\n            let pr ='';\r\n            let node = undefined;\r\n            do {\r\n                if (!node) node = dd.parent; else node=node.parent;\r\n                if(node.label) pr+=`${node.label};`\r\n            } while (node)\r\n            console.log(pr);\r\n            event.preventDefault();\r\n            this.menuX = event.clientX;\r\n            this.menuY = event.clientY;\r\n            this.showMenu = true;\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            defaultProps: {\r\n                children: 'children',\r\n                label: 'label',\r\n            },\r\n            menu: [],\r\n            menuOptions: [\r\n                { label: 'Edit', action: this.edit },\r\n                { label: 'Delete', action: this.delete }\r\n            ],\r\n            showMenu: false,\r\n            menuX: 0,\r\n            menuY: 0\r\n        }\r\n    }, mounted() {\r\n        this.getMenu();\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n#myContextMenu {\r\n    position: absolute;\r\n    background-color: red;\r\n    z-index: 999;\r\n    width: 100px;\r\n    height: 100px;\r\n    padding: 0;\r\n\r\n    ul {\r\n        padding: 0;\r\n        padding-top: 5px;\r\n        list-style-type: none;\r\n        align-content: center;\r\n\r\n        li {\r\n            cursor: pointer;\r\n        }\r\n    }\r\n}</style>"],"mappings":";AAYA,eAAe;EAEXA,OAAO,EAAE;IACLC,eAAeA,CAACC,IAAI,EAAEC,EAAE,EAAE;MACtBC,OAAO,CAACC,GAAG,CAAC,YAAY,EAAEH,IAAI,EAAEC,EAAE;IACtC,CAAC;IACDG,eAAeA,CAACC,YAAY,EAAEC,QAAQ,EAAEL,EAAE,EAAE;MACxCC,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAEG,QAAQ,CAACC,KAAK,EAAEN,EAAE;IACtD,CAAC;IACDO,eAAeA,CAACH,YAAY,EAAEC,QAAQ,EAAEL,EAAE,EAAE;MACxCC,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAEG,QAAQ,CAACC,KAAK,EAAEN,EAAE;IACtD,CAAC;IACDQ,cAAcA,CAACJ,YAAY,EAAEC,QAAQ,EAAEL,EAAE,EAAE;MACvCC,OAAO,CAACC,GAAG,C