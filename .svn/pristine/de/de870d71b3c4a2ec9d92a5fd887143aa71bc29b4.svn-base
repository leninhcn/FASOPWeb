his.$refs.btnShowEmoji.contains(event.target)) {\r\n        //   this.ShowEmoji();\r\n        // }\r\n      }\r\n    },\r\n    ShowEmoji() {\r\n      if (!this.EmojiVisible) {\r\n        this.$refs.emojiPicker.style.scale = \"1\";\r\n        document.addEventListener(\"click\", this.handleClickOutside);\r\n      } else {\r\n        this.$refs.emojiPicker.style.scale = \"0\";\r\n        document.removeEventListener(\"click\", this.handleClickOutside);\r\n      }\r\n      setTimeout(() => {\r\n        this.EmojiVisible = !this.EmojiVisible;\r\n      }, 100);\r\n    },\r\n    EnableSender(stt) {\r\n      this.connected = stt;\r\n    },\r\n    sendMessage() {\r\n      const msg = this.message.replace(\"\\n\", \"\");\r\n      this.message = \"\";\r\n      if (!msg) {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"Cannot send blank message!\",\r\n        });\r\n        return;\r\n      }\r\n      this.$emit(\"MessageSent\", msg);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.emojiPicker {\r\n  position: absolute;\r\n  bottom: 90px;\r\n  scale: 0;\r\n  transition: scale ease-in-out 0.3s;\r\n}\r\n</style>"],"mappings":"AAqDA,OAAOA,WAAU,MAAO,mBAAmB;AAC3C,eAAe;EACbC,QAAQ,EAAE,CAAC,CAAC;EACZC,UAAU,EAAE;IAAEF;EAAY,CAAC;EAC3BG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,aAAaA,CAAA,EAAG,CAAC,CAAC;EAClBC,OAAOA,CAAA,EAAG,CAAC,CAAC;EACZC,OAAO,EAAE;IACPC,kBAAkBA,CAACC,KAAK,EAAE;MACxB,IACE,IAAI,CAACC,KAAK,CAACC,WAAU,IACrB,CAAC,IAAI,CAACD,KAAK,CAACC,WAAW,CAACC,QAAQ,CAACH,KAAK,CAACI,MAAM,KAC7C,IAAI,CAACT,YAAW,EAChB;QACAU,OAAO,CAACC,GAAG,CAACN,KAAK,CAACI,MAAM,CAAC;QACzB;QACA;QACA;MACF;IACF,CAAC;;IACDG,SAASA,CAAA,EAAG;MACV,IAAI,CAAC,IAAI,CAACZ,YAAY,EAAE;QACtB,IAAI,CAACM,KAAK,CAACC,WAAW,CAACM,KAAK,CAACC,KAAI,GAAI,GAAG;QACxCC,QAAQ,CAACC,gBAAgB,CAAC,OAAO,EAAE,IAAI,CAACZ,kBAAkB,CAAC;MAC7D,OAAO;QACL,IAAI,CAACE,KAAK,CAACC,WAAW,CAACM,KAAK,CAACC