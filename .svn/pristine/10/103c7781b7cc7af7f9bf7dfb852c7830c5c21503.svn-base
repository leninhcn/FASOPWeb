{"ast":null,"code":"export default {\n  name: 'menu-bar'\n};","map":{"version":3,"names":["name"],"sources":["D:\\LearnVueJS\\Vue3\\Projects\\fa-sop-online\\src\\components\\Menu.vue"],"sourcesContent":["<template>\r\n  <div class=\"home_container\">\r\n    <el-container class=\"home_container\">\r\n      <el-header>\r\n        <div>\r\n          <img src=\"../assets/logo.png\" alt=\"\" />\r\n          <span>BU23 MES</span>\r\n        </div>\r\n        <div>\r\n          <el-dropdown @command=\"handleCommand\" trigger=\"click\">\r\n            <span class=\"el-dropdown-link\">\r\n              {{ lang }}<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n            </span>\r\n            <el-dropdown-menu slot=\"dropdown\" align=\"center\">\r\n              <el-dropdown-item style=\"min-width: 60px\" command=\"中文\">中文</el-dropdown-item>\r\n              <el-dropdown-item style=\"min-width: 60px\" command=\"英文\">英文</el-dropdown-item>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n          <el-button type=\"info\" @click=\"logout\">退出</el-button>\r\n        </div>\r\n      </el-header>\r\n      <el-container>\r\n        <el-aside :width=\"IsCollapse ? '64px' : '200px'\">\r\n          <div class=\"toggle-button\" @click=\"toggleCollapse\" id=\"toggle-button\">\r\n            <i :class=\"\r\n                IsCollapse ? 'iconfont iconchtab' : 'iconfont iconthumbnail'\r\n              \"></i>\r\n          </div>\r\n\r\n          <!-- 侧边栏-->\r\n          <el-menu background-color=\"#333744\" text-color=\"#fff\" active-text-color=\"#409eff\" :unique-opened=\"true\" :collapse=\"IsCollapse\" :collapse-transition=\"false\" :router=\"true\" :default-active=\"activePath\">\r\n            <!-- 1级菜单-->\r\n            <el-submenu :index=\"item.Id + ' '\" v-for=\"item in menuList\" :key=\"item.Id\">\r\n              <!--1级菜单模板-->\r\n              <template slot=\"title\">\r\n                <i :class=\"iconsObj[item.Id]\"></i>\r\n                <span>{{ item.MenuName }}</span>\r\n              </template>\r\n              <!-- 2级菜单-->\r\n              <el-menu-item :index=\"subItem.Path == '' ? subItem.Id + ' ' : subItem.Path\" v-for=\"subItem in item.Chlid\" :key=\"subItem.Id\" @click=\"saveNavState(subItem.Path)\">\r\n                <template slot=\"title\">\r\n                  <i class=\"el-icon-menu\"></i>\r\n                  <span>{{ subItem.MenuName }}</span>\r\n                </template>\r\n              </el-menu-item>\r\n            </el-submenu>\r\n          </el-menu>\r\n        </el-aside>\r\n        <el-main>\r\n          <!-- 路由占位符-->\r\n          <router-view></router-view>\r\n        </el-main>\r\n      </el-container>\r\n    </el-container>\r\n    <div>\r\n      <el-drawer title=\"用户信息补充\" :before-close=\"handleClose\" :with-header=\"false\" :close-on-click-modal=\"false\" :modal=\"false\" :close-on-press-escape=\"false\" :visible.sync=\"show_drawer\" direction=\"ltr\" custom-class=\"drawer\" ref=\"drawer\">\r\n        <span>用户信息补充</span>\r\n        <div class=\"drawer_content\">\r\n          <el-form :model=\"MoreInfoForm\" :rules=\"addMoreInfoFormRules\" ref=\"addMoreInfoFormRef\">\r\n            <el-form-item label=\"工号\" :label-width=\"formLabelWidth\">\r\n              <el-input v-model=\"MoreInfoForm.ACCOUNT\" :disabled=\"true\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"姓名\" prop=\"CNAME\" :label-width=\"formLabelWidth\">\r\n              <el-input v-model=\"MoreInfoForm.CNAME\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"邮箱\" prop=\"EMAIL\" :label-width=\"formLabelWidth\">\r\n              <el-input v-model=\"MoreInfoForm.EMAIL\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"厂区\" prop=\"PLANTID\" :label-width=\"formLabelWidth\">\r\n              <el-select v-model=\"MoreInfoForm.PLANTID\" placeholder=\"请选择地点\" style=\"width: 100%\">\r\n                <el-option v-for=\"item in location_option\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-form>\r\n          <div class=\"drawer__footer\">\r\n            <el-button @click=\"cancelForm\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"$refs.drawer.closeDrawer()\" :loading=\"loading\">{{ loading ? \"提交中 ...\" : \"确 定\" }}</el-button>\r\n          </div>\r\n        </div>\r\n      </el-drawer>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\nname:'menu-bar'\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.el-header {\r\n  background-color: #37314d;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding-left: 0%;\r\n  align-items: center;\r\n  color: #fff;\r\n  font-size: 20px;\r\n}\r\n\r\n.el-header > div {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.el-header > div > span {\r\n  margin-left: 15px;\r\n}\r\n\r\n.drawer__footer {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n/* .el-header>div>img {\r\n     width: 150px;\r\n    border-radius: 50%;\r\n    background-color: #eee\r\n  } */\r\n\r\n.el-aside {\r\n  background-color: #333744;\r\n}\r\n\r\n.el-aside > .el-menu {\r\n  /* 取消右边不对齐 */\r\n  border-right: none;\r\n}\r\n\r\n.el-main {\r\n  background-color: #eaedf1;\r\n}\r\n\r\n.home_container {\r\n  height: 100%;\r\n}\r\n\r\n.el-submenu {\r\n  text-align: left;\r\n}\r\n\r\n.iconfont {\r\n  margin-right: 10px;\r\n}\r\n\r\n.toggle-button {\r\n  background-color: #4a5064;\r\n  font-size: 10px;\r\n  line-height: 20px;\r\n  color: #fff;\r\n  text-align: center;\r\n  letter-spacing: 0.2em;\r\n  cursor: pointer;\r\n  width: auto;\r\n}\r\n\r\n.el-button {\r\n  margin-left: 15px;\r\n}\r\n\r\n.drawer {\r\n  padding-top: 10px;\r\n  span {\r\n    text-align: center;\r\n  }\r\n}\r\n.drawer_content {\r\n  padding: 20px;\r\n}\r\n\r\n.tableau-icon {\r\n  border-radius: 50%;\r\n  /* border: solid 1px #eee; */\r\n  /* box-shadow: 0 0 10px #ddd; */\r\n  width: 20px;\r\n  height: 20px;\r\n  padding-left: 15px;\r\n}\r\n\r\n.tableau-icon:hover {\r\n  cursor: pointer;\r\n}\r\n\r\n.el-col {\r\n  padding: 10px;\r\n}\r\n\r\n.el-icon-position {\r\n  color: #eee;\r\n  size: 50px;\r\n}\r\n\r\n.drop_item {\r\n  font-size: 20px;\r\n}\r\n\r\n.el-icon-position {\r\n  cursor: pointer;\r\n}\r\n</style>"],"mappings":"AAqFA,eAAe;EACfA,IAAI,EAAC;AACL"},"metadata":{},"sourceType":"module","externalDependencies":[]}