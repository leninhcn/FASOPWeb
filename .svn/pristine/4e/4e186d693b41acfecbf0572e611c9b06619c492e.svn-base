show=\"repeat\"\r\n              class=\"numOfRP\"\r\n              :class=\"{ extraControlActive: repeat }\"\r\n            >\r\n              {{ repeatNum }}\r\n            </p>\r\n          </div>\r\n          <i\r\n            class=\"fa-solid fa-shuffle\"\r\n            :class=\"{ extraControlActive: shuffle }\"\r\n            @click=\"ShuffleTrack\"\r\n          ></i>\r\n          <i\r\n            class=\"fa-solid fa-bars-staggered\"\r\n            :class=\"{ extraControlActive: !lstCollapse }\"\r\n            @click=\"ShowList\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nexport default {\r\n  name: \"AudioPlayer\",\r\n  props: {\r\n    src: {\r\n      type: String,\r\n      required: false,\r\n    },\r\n    lstCollapse: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: true,\r\n    },\r\n    trackList: {\r\n      type: Array,\r\n      required: true,\r\n      default: [\r\n        {\r\n          src: \"\",\r\n          title: \"chan lam roi\",\r\n          current_time: 0,\r\n          totaltime: 0,\r\n          idx: -1,\r\n          DB_ID: 0,\r\n        },\r\n      ],\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      shuffle: false,\r\n      repeat: false,\r\n      repeatNum: undefined,\r\n      l2play: true,\r\n      trackinfo: {\r\n        src: \"\",\r\n        title: \"chan lam roi\",\r\n        current_time: 0,\r\n        totaltime: 0,\r\n        idx: -1,\r\n        DB_ID: 0,\r\n      },\r\n      ttvisible: false,\r\n      tooltipRef: {\r\n        getBoundingClientRect: () => this.position,\r\n      },\r\n      isPlaying: false,\r\n      //  userPaused: false,\r\n      position: {\r\n        top: 0,\r\n        left: 0,\r\n        bottom: 0,\r\n        right: 0,\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    var parent = this;\r\n    this.$refs.player.addEventListener(\"pause\", function () {\r\n      if (parent.isPlaying) parent.isPlaying = false;