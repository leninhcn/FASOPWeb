visible\"\r\n            :content=\"timetostring(trackinfo.current_time)\"\r\n            placement=\"top\"\r\n            effect=\"light\"\r\n            trigger=\"click\"\r\n            virtual-triggering\r\n            :virtual-ref=\"tooltipRef\"\r\n          />\r\n\r\n          <div class=\"mainPlayer\">\r\n            <input\r\n              ref=\"pcTime\"\r\n              @mousedown=\"pg_mouseDown\"\r\n              @mouseup=\"pg_mouseUp\"\r\n              @mousemove=\"valueOnMouse\"\r\n              type=\"range\"\r\n              name=\"myRange\"\r\n              id=\"myRange\"\r\n              min=\"0\"\r\n              :max=\"Math.floor(trackinfo.totaltime)\"\r\n              class=\"slider\"\r\n              v-model=\"trackinfo.current_time\"\r\n            />\r\n\r\n            <div class=\"btnGroupControl\">\r\n              <button class=\"btnPrev\" @click=\"PreviousPlay\">\r\n                <i class=\"fa-solid fa-backward-fast\"></i>\r\n              </button>\r\n              <button class=\"btnPlay\" @click=\"play\">\r\n                <i v-if=\"!isPlaying\" class=\"fa-solid fa-play\"></i>\r\n                <i v-if=\"isPlaying\" class=\"fa-solid fa-pause\"></i>\r\n              </button>\r\n              <button class=\"btnNext\" @click=\"NextPlay\">\r\n                <i class=\"fa-solid fa-forward-fast\"></i>\r\n              </button>\r\n              <button class=\"btnMarquee\">\r\n                <marquee scrollamount=\"2\">{{ trackinfo.title }}</marquee>\r\n              </button>\r\n            </div>\r\n\r\n            <div class=\"showTime\">\r\n              <p style=\"display: inline\">\r\n                {{ timetostring(trackinfo.current_time) }}\r\n              </p>\r\n              <p style=\"display: inline\">\r\n                / {{ timetostring(trackinfo.totaltime) }}\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </td>\r\n    </tr>\r\n  </div>\r\n  <div ref=\"DocList\" class=\"DocList\">\r\n    <button class=\"btnShowList\" @click=\"ShowList\">Show List</button>\r\n    <div ref=\"listCard\" class=\"listCard\" v-if=\"lstCollapse\">\r\n      <tr>\r\n        <td style=\"height: 180px\">\r\n          <div style=\"height: 100%; overflow-y: auto\">\r\n            <ul v-for=\"(track, id) in trackList\" :key=\"id\">\r\n              <li @click=\"PlayFromList(track.ID)\">\r\n                <i class=\"fa-solid fa-music\"></i> {{ track.FILE_NAME }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </td>\r\n      </tr>\r\n      <hr />\r\n      <tr style=\"height: 60px\">\r\n        <td>\r\n          <div style=\"overflow-y: auto\">\r\n            <ul v-for=\"(cata, id) in CatagoryList\" :key=\"id\">\r\n              <li>{{ cata }}</li>\r\n            </ul>\r\n          </div>\r\n        </td>\r\n      </tr>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nexport default {\r\n  name: \"AudioPlayer\",\r\n  props: {\r\n    src: {\r\n      type: String,\r\n      required: false,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      lstCollapse: true,\r\n      trackList: [],\r\n      CatagoryList: [\"CAT1\"],\r\n      playing_src: \"\",\r\n      l2play: true,\r\n      trackinfo: {\r\n        src: \"\",\r\n        title: \"chan lam roi\",\r\n        current_time: 0,\r\n        totaltime: 0,\r\n        idx: 0,\r\n      },\r\n      ttvisible: false,\r\n      tooltipRef: {\r\n        getBoundingClientRect: () => this.position,\r\n      },\r\n      isPlaying: false,\r\n      position: {\r\n        top: 0,\r\n        left: 0,\r\n        bottom: 0,\r\n        right: 0,\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.GetAllCatagory();\r\n    var parent = this;\r\n    this.$refs.player.addEventListener(\"pause\", function () {\r\n      if (parent.isPlaying) parent.isPlaying = false;\r\n      console.log(\"paused!!!\");\r\n    });\r\n    this.$refs.player.addEventListener(\"play\", function () {\r\n      if (!parent.isPlaying) parent.isPlaying = true;\r\n      console.log(\"played!!!\");\r\n    });\r\n    this.$refs.player.addEventListener(\"seeked\", function () {\r\n      this.play();\r\n      console.log(\"seeked!!!\");\r\n    });\r\n  },\r\n  methods: {\r\n    async GetAllCatagory() {\r\n      await this.axios\r\n        .get(\"api/Funapp/GetAllCatagory\")\r\n        .then((res) => {\r\n          this.CatagoryList = res.data;\r\n          if (this.trackList.length < 1) this.GetDocList(this.CatagoryList[0]);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err.response.data);\r\n        });\r\n    },\r\n    PreviousPlay() {\r\n      const temp = this.trackinfo.idx - 1;\r\n      if (temp >= 0) this.PlayFromList(this.trackList[temp].ID);\r\n    },\r\n    NextPlay() {\r\n      const temp = this.trackinfo.idx + 1;\r\n      if (temp < this.trackList.length)\r\n        this.PlayFromList(this.trackList[temp].ID);\r\n    },\r\n    PlayFromList(id) {\r\n      if (!this.$refs.player.paused) this.pause();\r\n      this.trackList.forEach((el, idx) => {\r\n        if (el.ID == id) {\r\n          this.trackinfo = {\r\n            src: `${this.axios.defaults.baseURL}api/Funapp/StreamAudio?id=${el.ID}`,\r\n            title: el.FILE_NAME,\r\n            current_time: 0,\r\n            totaltime: 0,\r\n            idx: idx,\r\n          };\r\n          this.l2play = true;\r\n          this.play();\r\n          return;\r\n        }\r\n      });\r\n    },\r\n    async GetDocList(cata) {\r\n      await this.axios\r\n        .get(`api/Funapp/GetDocList?cata=${cata}`)\r\n        .then((res) => {\r\n          this.trackList = res.data;\r\n        })\r\n        .catch((err) => {\r\n          this.$message({\r\n            showClose: true,\r\n            type: \"error\",\r\n            message: err.response.data,\r\n          });\r\n        });\r\n    },\r\n    ShowList() {\r\n      if (this.lstCollapse) {\r\n        this.$refs.DocList.style.right = \"0\";\r\n\r\n      } else {\r\n