\r\n        <div class=\"btnGroupControl\">\r\n          <button class=\"btnPrev\" @click=\"PreviousPlay\">\r\n            <i class=\"fa-solid fa-backward-fast\"></i>\r\n          </button>\r\n          <button class=\"btnPlay\" @click=\"play\">\r\n            <i v-if=\"!isPlaying\" class=\"fa-solid fa-play\"></i>\r\n            <i v-if=\"isPlaying\" class=\"fa-solid fa-pause\"></i>\r\n          </button>\r\n          <button class=\"btnNext\" @click=\"NextPlay\">\r\n            <i class=\"fa-solid fa-forward-fast\"></i>\r\n          </button>\r\n          <button class=\"btnMarquee\">\r\n            <marquee scrollamount=\"2\">{{ trackinfo.title }}</marquee>\r\n          </button>\r\n        </div>\r\n\r\n        <div class=\"showTime\">\r\n          <p style=\"display: inline\">\r\n            {{ timetostring(trackinfo.current_time) }}\r\n          </p>\r\n          <p style=\"display: inline\">\r\n            / {{ timetostring(trackinfo.totaltime) }}\r\n          </p>\r\n        </div>\r\n        <div class=\"extraControl\">\r\n          <div style=\"position: relative; display: inline-block\">\r\n            <i\r\n              class=\"fa-solid fa-repeat\"\r\n              :class=\"{ extraControlActive: repeat }\"\r\n              @click=\"RepeatTrack\"\r\n            ></i>\r\n            <p\r\n              v-show=\"repeat\"\r\n              class=\"numOfRP\"\r\n              :class=\"{ extraControlActive: repeat }\"\r\n            >\r\n              {{ repeatNum }}\r\n            </p>\r\n          </div>\r\n          <i\r\n            class=\"fa-solid fa-shuffle\"\r\n            :class=\"{ extraControlActive: shuffle }\"\r\n            @click=\"ShuffleTrack\"\r\n          ></i>\r\n          <i\r\n            class=\"fa-solid fa-bars-staggered\"\r\n            :class=\"{ extraControlActive: !lstCollapse }\"\r\n            @click=\"ShowList\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div ref=\"DocList\" class=\"DocList\">\r\n    <button class=\"btnShowList\" @click=\"ShowList\">Show List</button>\r\n    <div ref=\"listCard\" class=\"listCard\" v-if=\"!lstCollapse\">\r\n      <tr>\r\n        <td style=\"height: 180px\">\r\n          <div style=\"height: 100%; overflow-y: auto\">\r\n            <ul v-for=\"(track, id) in trackList\" :key=\"id\">\r\n              <li\r\n                @click=\"PlayFromList(track.ID)\"\r\n                :class=\"{\r\n                  playingTrack:\r\n                    trackList[trackinfo.idx] &&\r\n                    track.ID == trackList[trackinfo.idx].ID,\r\n                }\"\r\n              >\r\n                <i\r\n                  class=\"fa-solid\"\r\n                  :class=\"{\r\n                    'fa-music':\r\n                      !trackList[trackinfo.idx] ||\r\n                      track.ID != trackList[trackinfo.idx].ID,\r\n                    'fa-compact-disc':\r\n                      trackList[trackinfo.idx] &&\r\n                      track.ID == trackList[trackinfo.idx].ID,\r\n                    'fa-spin':\r\n                      trackList[trackinfo.idx] &&\r\n                      track.ID == trackList[trackinfo.idx].ID,\r\n                  }\"\r\n                ></i>\r\n                {{ track.FILE_NAME }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </td>\r\n      </tr>\r\n      <hr />\r\n      <tr style=\"height: 60px\">\r\n        <td>\r\n          <div style=\"overflow-y: auto\">\r\n            <ul v-for=\"(cata, id) in CatagoryList\" :key=\"id\">\r\n              <li\r\n                @click=\"GetDocList(cata)\"\r\n                :class=\"{\r\n                  playingTrack: cata == CurrentCata,\r\n                }\"\r\n              >\r\n                <i class=\"fa-solid fa-list-ul\"></i>\r\n                {{ cata }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </td>\r\n      </tr>\r\n    </div>\r\n    <button @click=\"()=>console.log(repeatNum)\">aaaa</button>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nexport default {\r\n  name: \"AudioPlayer\",\r\n  props: {\r\n    src: {\r\n      type: String,\r\n      required: false,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      shuffle: false,\r\n      repeat: false,\r\n      repeatNum: undefined,\r\n      CurrentCata: \"\",\r\n      lstCollapse: true,\r\n      trackList: [],\r\n      CatagoryList: [\"CAT1\"],\r\n      playing_src: \"\",\r\n      l2play: true,\r\n      trackinfo: {\r\n        src: \"\",\r\n        title: \"chan lam roi\",\r\n        current_time: 0,\r\n        totaltime: 0,\r\n        idx: -1,\r\n      },\r\n      ttvisible: false,\r\n      tooltipRef: {\r\n        getBoundingClientRect: () => this.position,\r\n      },\r\n      isPlaying: false,\r\n      //  userPaused: false,\r\n      position: {\r\n        top: 0,\r\n        left: 0,\r\n        bottom: 0,\r\n        right: 0,\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.GetAllCatagory();\r\n    var parent = this;\r\n    this.$refs.player.addEventListener(\"pause\", function () {\r\n      if (parent.isPlaying) parent.isPlaying = false;\r\n    });\r\n    this.$refs.player.addEventListener(\"play\", function () {\r\n      if (!parent.isPlaying) parent.isPlaying = true;\r\n      parent.userPaused = false;\r\n    });\r\n    this.$refs.player.addEventListener(\"ended\", function () {\r\n      parent.NextPlay();\r\n    });\r\n    this.$refs.player.addEventListener(\"seeked\", function () {\r\n      this.play();\r\n    });\r\n  },\r\n  methods: {\r\n    RepeatTrack() {\r\n      if (!this.repeat || this.repeatNum != \"A\") this.repeat = true;\r\n      else {\r\n        console.log(this.repeatNum);\r\n        this.repeat = false;\r\n      }\r\n      this.repeatNum = this.repeatNum == \"1\" ? \"A\" : \"1\";\r\n      // if (!this.repeatNum) this.repeatNum = \"1\";\r\n      // else if (this.repeatNum == \"1\") this.repeatNum = \"A\";\r\n    },\r\n    async GetAllCatagory() {\r\n      await this.axios\r\n        .get(\"api/Funapp/GetAllCatagory\")\r\n        .then((res) => {\r\n          this.CatagoryList = res.data;\r\n          if (this.trackList.length < 1) this.GetDocList(this.CatagoryList[0]);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err.response.data);\r\n        });\r\n    },\r\n    PreviousPlay() {\r\n      const temp = this.trackinfo.idx - 1;\r\n      if (temp >= 0) this.PlayFromList(this.trackList[temp].ID);\r\n    },\r\n    NextPlay() {\r\n      const temp = this.trackinfo.idx + 1;\r\n      if (temp < this.trackList.length)\r\n        this.PlayFromList(this.trackList[temp].ID);\r\n    },\r\n    PlayFromList(id) {\r\n      if (!this.$refs.player.paused) this.pause();\r\n      this.trackList.forEach((el, idx) => {\r\n        if (el.ID == id) {\r\n          this.trackinfo = {\r\n            src: `${this.axios.defaults.baseURL}api/Funapp/StreamAudio?id=${el.ID}`,\r\n            title: el.FILE_NAME,\r\n            current_time: 0,\r\n            totaltime: 0,\r\n            idx: idx,\r\n          };\r\n          this.l2play = true;\r\n          setTimeout(() => this.play(), 100);\r\n          return;\r\n        }\r\n      });\r\n    },\r\n    async GetDocList(cata) {\r\n      if (cata != this.CurrentCata) this.CurrentCata = cata;\r\n      else return;\r\n      await this.axios\r\n        .get(`api/Funapp/GetDocList?cata=${cata}`)\r\n        .then((res) => {\r\n          this.trackList = res.data;\r\n        })\r\n        .catch((err) => {\r\n          this.$message({\r\n            showClose: true,\r\n            type: \"error\",\r\n            message: err.response.data,\r\n          });\r\n        });\r\n    },\r\n    ShowList() {\r\n      if (this.lstCollapse) {\r\n        this.$refs.DocList.style.width = \"500px\";\r\n        setTimeout(() => {\r\n          this.lstCollapse = false;\r\n        }, 600);\r\n      } else {\r\n        this.$refs.DocList.style.width = \"40px\";\r\n        setTimeout(() => {\r\n          this.lstCollapse = true;\r\n        }, 600);\r\n      }\r\n    },\r\n    play() {\r\n      if (this.trackinfo.idx == -1) {\r\n        this.NextPlay();\r\n        return;\r\n      }\r\n\r\n      if (!this.isPlaying) {\r\n        if (this.trackinfo.src) {\r\n          if (this.l2play) {\r\n            this.$refs.player.load();\r\n            this.l2play = false;\r\n          }\r\n          this.$refs.player.play();\r\n        }\r\n      } else this.pause();\r\n    },\r\n    ShowCurrentTime() {\r\n      if (this.ttvisible) return;\r\n      if (this.$refs.player)\r\n        this.trackinfo.current_time = this.$refs.player.currentTime;\r\n      if (this.$refs.player)\r\n        this.trackinfo.totaltime = this.$refs.player.duration;\r\n    },\r\n    pause() {\r\n      this.userPaused = true;\r\n      this.$refs.player.pause();\r\n    },\r\n    pg_mouseDown() {\r\n      this.ttvisible = true;\r\n    },\r\n    pg_mouseUp() {\r\n      this.ttvisible = false;\r\n      this.$refs.player.currentTime = this.trackinfo.current_time;\r\n    },\r\n    valueOnMouse: function (e) {\r\n      if (this.ttvisible) {\r\n        const rec = this.$refs.pcTime.getBoundingClientRect();\r\n        this.position = DOMRect.fromRect({\r\n          width: 10,\r\n          height: 10,\r\n          x: e.clientX,\r\n          y: rec.top,\r\n        });\r\n      }\r\n    },\r\n    timetostring: function (time) {\r\n      var sec_num = parseInt(time, 10);\r\n      var hours = Math.floor(sec_num / 3600);\r\n      var minutes = Math.floor((sec_num - hours * 3600) / 60);\r\n      var seconds = sec_num - hours * 3600 - minutes * 60;\r\n\r\n      if (hours < 10) {\r\n        hours = \"0\" + hours;\r\n      }\r\n      if (minutes < 10) {\r\n        minutes = \"0\" + minutes;\r\n      }\r\n      if (seconds < 10) {\r\n        seconds = \"0\" + seconds;\r\n      }\r\n      return hours + \":\" + minutes + \":\" + seconds;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r